<script>
  $(document).ready(function() {
    $('.notice').css({'position': 'absolute', 'top': '75px'})
                .hide();
    rssFeed();
    twitterFeed();
  })
</script>

<div class="notice"></div>

<div class="rss feed" >
  <h2>RSS feed</h2>

  <% @feeds.each do |feed| %>
    <div data-feed="<%= feed %>"></div>
  <% end %>
</div>


<div class="twitter feed">
  <h2>Social feed</h2>
  <img src="assets/loader2.gif" alt="" id='loader' />
</div>
