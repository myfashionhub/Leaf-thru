<div id="user_nav">
     <% if user_signed_in? %>
          <img src="<%= user_avatar %>" id="main_avatar"> Signed in as <%= current_user.email %>.<br />
          Not you? 
     <% else %>
          <%= link_to "Sign In", new_user_session_path %>
          <%= link_to "Sign Up", new_user_registration_path %>
     <% end %>
</div>
